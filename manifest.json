{
  "manifest_version": 3,
  "name": "Hello Extensions",
  "description": "Something simple",
  "version": "1.0",
  "action": {
    "default_popup": "hello.html",
    "default_icon": "./images/hello.png"
  },
  "content_scripts": [
    {
      "js": ["./scripts/content.js"],
      "matches": ["https://192.168.100.1/*"]
    }
  ]
}
